     //- most view articles
     aside.widget
      h3.widget-title יומן הבניה היחיד בישראל.
      .popular-posts-wrapper.pdt10
       each post , i in posts['posts']
        if i<21
          .post
           .post-image
            a(href="/blog/#{post.slug}") 
             if post.image
              img(height="60" width="60" src="/blog#{post.image}" alt="#{post.title}")
             else
              img(height="60" width="60" src="http://placehold.it/60x60/f3f3f3/0f0f0f&text=bonimbayit.co.il" alt="#{post.title}")
           .post-content
            a(href="#")=post.title.substr(0, 70)
